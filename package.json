        "credentials": [
            "dist/credentials/DremioApi.credentials.js"
        ]
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "copyfiles": "^2.4.1",
        "eslint": "^8.57.0",
        "n8n-core": "^1.0.0",
        "n8n-workflow": "^1.0.0",
        "prettier": "^2.0.5",
        "typescript": "^5.0.0"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
