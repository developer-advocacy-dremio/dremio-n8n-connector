{
    "name": "n8n-nodes-dremio",
    "version": "2.0.13",
    "description": "n8n node for Dremio",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "https://github.com/developer-advocacy-dremio/dremio-n8n-connector",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/developer-advocacy-dremio/dremio-n8n-connector.git"
    },
    "author": {
        "name": "Alex Merced",
        "email": "alex.merced@dremio.com"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && copyfiles -u 1 \"nodes/**/*.svg\" dist/",
        "dev": "tsc --watch",
        "test": "jest",
        "lint": "eslint nodes/ credentials/ --ext .ts",
        "prepublishOnly": "npm run build"
